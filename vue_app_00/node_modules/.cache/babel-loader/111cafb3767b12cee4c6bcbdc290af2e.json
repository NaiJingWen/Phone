\n          _this.$store.commit(\"increment\", count); //共享数据不能刷新\n          //localStore 一个浏览器自带的对象 可以永久保存数据\n\n\n          Toast(result.body.msg);\n        } else {\n          Toast(result.body.msg);\n        }\n      }); //2.更新App.vue组件中购物车的数量\n    },\n    getGoodInfo: function getGoodInfo() {\n      var _this2 = this;\n\n      var id = this.$route.params.id;\n      this.$http.get(\"goodsinfo?id=\" + id).then(function (result) {\n        _this2.info = result.body; //console.log(this.info);\n      });\n    },\n    goodAdd: function goodAdd() {\n      if (this.val <= 999) {\n        this.val++;\n      }\n    },\n    goodSub: function goodSub() {\n      if (this.val > 1) this.val--;\n    },\n    //服务器返回模拟数据[id;title;now;old;商品编号]\n    //参数:商品id\n    getImageList: function getImageList() {\n      var _this3 = this;\n\n      this.$http.get(\"imagelist\").then(function (result) {\n        _this3.imglist = result.body;\n      });\n    }\n  },\n  created: function created() {\n    this.getImageList();\n    this.getGoodInfo();\n    console.log(this.$route.params.id);\n  },\n  components: {\n    \"swiper-box\": swiper\n  }\n};",{"version":3,"sources":["GoodInfo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmDA;AACA;AACA,OAAA,MAAA,MAAA,mBAAA,C,CACA;AACA;;AACA,SAAA,KAAA,QAAA,SAAA,C,CACA;AACA;;AACA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,eAAA,EADA;AAEA,WAAA,CAFA;AAGA,YAAA;AAHA,KAAA;AAKA,GAPA;AAQA,WAAA;AACA,aADA,uBACA;AAAA;;AACA;AACA;AACA;AACA,UAAA,KAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,CAJA,CAKA;;AACA,UAAA,QAAA,KAAA,GAAA,CANA,CAOA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,GAAA,SAAA,GAAA,KAAA,EAAA,IAAA,CAAA,kBAAA;AACA,YAAA,OAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA;AACA;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA,KAAA,EAHA,CAIA;AACA;;;AACA,gBAAA,OAAA,IAAA,CAAA,GAAA;AACA,SAPA,MAOA;AACA,gBAAA,OAAA,IAAA,CAAA,GAAA;AACA;AACA,OAXA,EARA,CAoBA;AACA,KAtBA;AAuBA,eAvBA,yBAuBA;AAAA;;AACA,UAAA,KAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,kBAAA,EAAA,EAAA,IAAA,CAAA,kBAAA;AACA,eAAA,IAAA,GAAA,OAAA,IAAA,CADA,CAEA;AACA,OAHA;AAIA,KA7BA;AA8BA,WA9BA,qBA8BA;AACA,UAAA,KAAA,GAAA,IAAA,GAAA,EAAA;AACA,aAAA,GAAA;AACA;AACA,KAlCA;AAmCA,WAnCA,qBAmCA;AACA,UAAA,KAAA,GAAA,GAAA,CAAA,EACA,KAAA,GAAA;AACA,KAtCA;AAuCA;AACA;AACA,gBAzCA,0BAyCA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,WAAA,EAAA,IAAA,CAAA,kBAAA;AACA,eAAA,OAAA,GAAA,OAAA,IAAA;AACA,OAFA;AAGA;AA7CA,GARA;AAuDA,SAvDA,qBAuDA;AACA,SAAA,YAAA;AACA,SAAA,WAAA;AACA,YAAA,GAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,GA3DA;AA4DA,cAAA;AACA,kBAAA;AADA;AA5DA,CAAA","sourcesContent":["<template>\r\n    <div class=\"app-goodinfo\">\r\n        <!--1.商品轮播区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <swiper-box :list=\"imglist\"></swiper-box>\r\n                </div>\r\n            </div>\r\n        </div>        \r\n        <!--2.商品购买区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">{{info.title}}</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p class=\"price\">\r\n                        市场价:<del>￥{{info.old}}</del>\r\n                        销售价:<span class=\"now\">￥{{info.now}}</span>\r\n                    </p>\r\n                    <p class=\"price\">\r\n                        购买的数量:\r\n                        <div class=\"mui-numbox\" data-numbox-min='1' data-numbox-max='9'>\r\n                            <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"goodSub\">-</button>\r\n                            <input id=\"test\" class=\"mui-input-numbox\" type=\"number\" value=\"1\" v-model=\"val\"/>\r\n                            <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"goodAdd\">+</button>\r\n                        </div>                        \r\n                    </p>\r\n                    <p>\r\n                        <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n                        <mt-button type=\"danger\" size=\"small\" @click=\"addCartTo\">加入购物车</mt-button>                        \r\n                    </p>                     \r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\"></div>\r\n        </div>        \r\n        <!--3.商品的参数区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">商品的参数</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p>商品货号:{{info.pid}}</p>\r\n                    <p>商品运费:0</p>\r\n                    <p>商品颜色:黑色</p>\r\n                    <p>商品尺寸:100m</p>                    \r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\"></div>\r\n        </div>                \r\n    </div>\r\n</template>\r\n<script>\r\n    //父组件 GoodInfo.vue\r\n    //1.引入\r\n    import swiper from '../sub/swiper.vue';\r\n    //2.引入mui的js库\r\n    // import mui from '../../lib/mui/js/mui.js'\r\n    import {Toast} from 'mint-ui';\r\n    //2.注册\r\n    //3.调用\r\n    export default {\r\n        data(){\r\n            return{\r\n                imglist:[],\r\n                val:1,\r\n                info:{}\r\n            }\r\n        },\r\n        methods:{\r\n            addCartTo(){\r\n                //console.log(\"添加至购物车\");\r\n                //1.将商品编号和数量保存到服务器   发送一个ajax请求\r\n                   //1.1获取商品编号 商品编号已经有了在地址栏上\r\n                   var id=this.$route.params.id;                  \r\n                   //1.2获取商品数量\r\n                   var count=this.val;                   \r\n                   //1.3发送请求\r\n                   this.$http.get(\"addCart?pid=\"+id+\"&count=\"+count).then(result=>{\r\n                       if(result.body.code==1){\r\n                           //1.4更新购物车中商品的数量\r\n                           //修改Vuex共享数据\r\n                           this.$store.commit(\"increment\",count);\r\n                           //共享数据不能刷新\r\n                           //localStore 一个浏览器自带的对象 可以永久保存数据\r\n                           Toast(result.body.msg);\r\n                       }else{\r\n                           Toast(result.body.msg);\r\n                       }\r\n                   });\r\n                //2.更新App.vue组件中购物车的数量\r\n            },\r\n            getGoodInfo(){\r\n                var id=this.$route.params.id;\r\n                this.$http.get(\"goodsinfo?id=\"+id).then(result=>{\r\n                    this.info = result.body;\r\n                    //console.log(this.info);\r\n                })\r\n            },\r\n            goodAdd(){\r\n                if(this.val<=999){\r\n                this.val++;                    \r\n                }\r\n            },\r\n            goodSub(){\r\n                if(this.val>1)\r\n                this.val--;\r\n            },\r\n            //服务器返回模拟数据[id;title;now;old;商品编号]\r\n            //参数:商品id\r\n            getImageList(){\r\n                this.$http.get(\"imagelist\").then(result=>{\r\n                    this.imglist=result.body;\r\n                })\r\n            }\r\n        },\r\n        created() {\r\n            this.getImageList();\r\n            this.getGoodInfo();\r\n            console.log(this.$route.params.id);\r\n        },\r\n        components:{\r\n            \"swiper-box\":swiper\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    \r\n</style>"],"sourceRoot":"src\\components\\goods"}]}